#include <iostream>
using namespace std;
int  max(int,int);
int max (int a,int b)
{
   if (a>b)
   {
   cout<<"a  is  maximum"<<a<<endl;
   }
   else
{
    cout<<"b is maximum"<<b<<endl;

}
return 0;
}
int main ()
{
    int  a,b;
    cout<<"enter  a,b"<<endl;
    cin>>a>>b;
    max(a,b);
    return 0;
}

